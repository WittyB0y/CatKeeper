{
  "name": "catkeeper-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "tsc": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 ",
    "format": "prettier --no-error-on-unmatched-pattern --write src/**/*.{js,jsx,ts,tsx,scss,md,json} --config ./.prettierrc",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.27",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/react-navigation": "^3.4.0",
    "axios": "^1.6.3",
    "expo": "~49.0.15",
    "expo-image-picker": "~14.3.2",
    "expo-sqlite": "~11.3.3",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-icons": "^4.12.0",
    "react-redux": "^9.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-react": "^7.23.3",
    "@commitlint/cli": "^19.0.3",
    "@commitlint/types": "^19.0.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/react": "~18.2.14",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "react-native": "0.72.10",
    "typescript": "^5.1.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config-git-commit/.commitizen.cjs"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md,json}": [
      "npm run format",
      "git add -A ."
    ]
  },
  "private": true
}
